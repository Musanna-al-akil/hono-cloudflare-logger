{
  "name": "hono-cloudflare-logger",
  "version": "0.1.0-beta.0",
  "description": "cloudflare-workers first structured logging middleware for Hono.",
  "keywords": [
    "cloudflare-log",
    "cloudflare-logger",
    "cloudflare-workers",
    "hono",
    "hono-logger",
    "logger",
    "middleware",
    "ndjson",
    "structured-logging",
    "syslog"
  ],
  "homepage": "https://github.com/musanna/hono-cloudflare-logger#readme",
  "bugs": {
    "url": "https://github.com/musanna/hono-cloudflare-logger/issues"
  },
  "license": "MIT",
  "author": "musanna",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/musanna/hono-cloudflare-logger.git"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --minify --clean",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "npm run typecheck && oxlint . --deny-warnings",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "pack:dry-run": "env npm_config_cache=/tmp/npm-cache npm pack --dry-run",
    "prepublish:check": "npm run typecheck && npm run lint && npm run test && npm run build && npm run pack:dry-run",
    "prepublishOnly": "npm run prepublish:check",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^22.19.13",
    "hono": "^4.12.3",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "hono": "^4.0.0"
  },
  "engines": {
    "node": ">=22"
  }
}
